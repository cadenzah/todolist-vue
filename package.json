{
  "name": "boilerplate-vue",
  "version": "1.0.0",
  "description": "Vue.js boilerplate",
  "main": "main.js",
  "scripts": {
    "build": "npx webpack --config ./config/webpack.config.js --progress",
    "test:watch": "npx jest --watchAll --verbose",
    "test:coverage": "npx jest --watchAll --verbose --coverage",
    "dev": "npx webpack-dev-server --config ./config/webpack.config.dev.js --progress --open chrome",
    "storybook": "npx start-storybook -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cadenzah/boilerplate-vue.git"
  },
  "keywords": [
    "vuejs",
    "vue",
    "javascript",
    "webpack"
  ],
  "author": "cadenzah <cadenzah93@gmail.com> (https://github.com/cadenzah)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cadenzah/boilerplate-vue/issues"
  },
  "homepage": "https://github.com/cadenzah/boilerplate-vue#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/vue": "^6.1.10",
    "@vue/test-utils": "^1.1.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.1.0",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.5",
    "vue-router": "^3.4.9",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
